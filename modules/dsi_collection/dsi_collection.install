<?php/** * Implements hook_install(). */function dsi_collection_install($is_syncing) {  user_role_grant_permissions('seo', [    'add collection entities',    'administer collection entities',    'delete collection entities',    'edit collection entities',    'view published collection entities',    'view unpublished collection entities',  ]);}/** * Implements hook_update_N(). */function dsi_collection_update_9000(&$sandbox) {  user_role_grant_permissions('seo', [    'add collection entities',    'administer collection entities',    'delete collection entities',    'edit collection entities',    'view published collection entities',    'view unpublished collection entities',  ]);}