<?php/** * Implements hook_install(). */function seo_site_install($is_syncing) {  $filter_formats = \Drupal::entityTypeManager()->getStorage('filter_format')->loadMultiple();  foreach ($filter_formats as $filter_format) {    $filter_format->setFilterConfig('filter_image_resize', [      'status' => 1,      'weight' => 0,      'settings' => [        'image_locations' => [          'local' => 'local',          'remote' => 'remote',        ],      ],    ])->save();  }}/** * Implements hook_update_N(). */function seo_site_update_9001(&$sandbox) {  $filter_formats = \Drupal::entityTypeManager()->getStorage('filter_format')->loadMultiple();  foreach ($filter_formats as $filter_format) {    $filter_format->setFilterConfig('filter_image_resize', [      'status' => 1,      'weight' => 0,      'settings' => [        'image_locations' => [          'local' => 'local',          'remote' => 'remote',        ],      ],    ])->save();  }}