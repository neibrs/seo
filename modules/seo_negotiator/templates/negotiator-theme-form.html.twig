{{ form|without('themes_container') }}<div class="row py-1">  {% for name,item in form.themes_container %}    {% if '#' not in name %}      <div class="col-2">        <div class="tkdb-boxlist text-center">          <div class="boxlist-header">            {{ item.screen }}            {{ item.type }}          </div>          {{ item.machine_label }}          <p>            {{ item.domains }}          </p>          <p class="text-left">            {{ item.type_theme_action }}          </p>        </div>      </div>    {% endif %}  {% endfor %}</div>