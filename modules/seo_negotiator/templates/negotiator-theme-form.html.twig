{{ form|without('themes_container') }}<div class="row py-1">  {% for name,item in form.themes_container %}    {% if '#' not in name %}      <div class="col-2">        <div class="tkdb-boxlist text-center">          {{ item.screen }}          <h4 class="mb-0">{{ item.machine_label }}</h4>          <p>{{ item.machine_name }}</p>          <p>            {{ item.domains }}          </p>          <p class="text-left">            {{ item.type_theme_action }}          </p>        </div>      </div>    {% endif %}  {% endfor %}</div><li>  <p class="picbox">    <font class="theme_num">15</font>    <img src="./template/news/news02/preview.jpg">    <font class="theme_name">文章新闻</font>  </p>  <p class="name"><font color="blue">news02</font></p>  <p>绑定域名(一行一个)：</p>  <p><textarea style="width:150px;height:100px;overflow:auto;" wrap="off" name="domain"></textarea></p>  <p><button type="button" class="button sbtn">保存</button> <span class="returnmsg"></span></p></li>