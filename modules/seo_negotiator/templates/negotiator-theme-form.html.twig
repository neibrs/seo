{{ form|without('themes_container') }}<div class="row py-1">  {% for item in form.themes_container %}    {{ kint(item) }}    {% if item is empty %}    {% else %}      <div class="col-2">      <div class="tkdb-boxlist text-center">        <h4 class="mb-0">{{ item.type }}</h4>        <img src="{{ item.screen }}" alt="">        <p>{{ item.machine_name }}</p>        <p>{{ item.machine_label }}</p>        <p>          {{ item.domains }}        </p>        <p>          {{ item.type_theme_action }}        </p>      </div>    </div>    {% endif %}  {% endfor %}</div>